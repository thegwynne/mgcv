\section{Education}
\cvitem{2015--present}{Postgraduate Studies in Mathematics, \emph{University of Manchester} (not completed)}
       \cvitem{}{Working with an engineering company developing software to analyze expected vibration in buildings.}  % arguments 3 to 6 can be left empty
       \cvitem{2010--2014}{Masters of Mathematics, \emph{University of York}}
       \cvitem{}{\textit{First Class Honours}}
       \cvitem{}{Masters Dissertation: An Introduction to Bosonic String Theory}
       \cvitem{}{Modules focusing on applied mathematics and mathematical physics}
       \cvitem{2008--2010}{A Levels, \emph{Bishops' Bluecoat High School Sixth Form}}
       \cvitem{}{A* Mathematics, A* Further Mathematics, A Chemistry, A Physics}
       \cvitem{2003--2008}{GCSEs, \emph{Bishops' Bluecoat High School}}
       \cvitem{}{12 A*-C}
       
\section{Work Experience}
\cventry{2015--present}{Industrial Sponsor for PhD programme}{Arup}{Manchester}{}{}%
\cvitem{}{
\begin{itemize}%
\item Worked with Arup's Oasys software group members in Manchester and London,
\item Met with engineers from different disciplines to learn how they use vibration analysis, and discuss what improvements would be most valuable to them,
\item Constructed fully documented code to solve the Quadratic Eigenvalue Problem in \texttt{MATLAB},
  \item The code I developed is likely to be integrated into their structural analysis package later in the year.
\end{itemize}}
\cventry{Summer 2013}{Summer Placement}{Met Office}{Exeter}{}{}
\cvitem{}
    {
      \begin{itemize}        
      \item Developed a python module to compute \emph{Rossby Wave Source (RWS)}, a parameter of climate data, from meteorological datasets,
        \item Combined techniques from existing IDL code with novel algorithms to improve speed and utility,
    \item Presented mid-projects results at a group meeting.
    \end{itemize}
    }
\cventry{Summer 2009}{Nuffield Bursary Project}{QUASAR Group}{Cockroft Institute}{}{\begin{itemize}\item Completed four week project using finite element modelling software \item Shortlisted for an award at scheme poster presentation\end{itemize}}

\section{Training, Skills and Courses}
\subsection{Computing}
\cvitem{}{\begin{itemize}
  \item Scientific Computing MSC module at the University of Manchester, which included writing code in C++
  \item Introduction to Python, and Introduction to IDL courses at the Met Office
  \item Research in the last 3 years has primarily involved coding in \texttt{MATLAB}, as well as some work in Python and C++.
  \item Work at the Met Office was mostly conducted using python, as well as reading legacy IDL code.
    \item Personal coding projects are mostly completed in python, inluding election forecasting models, although for projects involving 3D graphics I tend to use C\# with the XNA package.
    \end{itemize}
}
\subsection{Other}
\cvitem{2015--2017}{Secretary of Manchester SIAM-IMA Student Chapter}
\cvitem{}{This was a postgraduate student society sponsored by the Society for Industrial and Applied Mathematics (SIAM) and the Institude of Mathematics and its Applications (IMA) to run events at the University of Manchester}
\cvitem{}{As secretary, my responsibilities included communications between members and committee, helping to plan and run events, and providing details of our operations to SIAM}
\cvitem{January 2017}{Presented a poster presentation at the SIAM UKIE Annual Meeting at the University of Strathclyde,}
\cvitem{September 2016}{Presented a talk at the 5th IMA Conference on Numerical Linear Algebra and Optimisation at the University of Birmingham,}
\cvitem{June 2016}{Completed the Graduate Teaching Assistant Training one day course at the University of Manchester.}

\section{Voluntary and Casual Work}
\cvitem{2015--present}{Holy Trinity Platt Church. Active volunteer in a number of areas, including in technical and support areas such as:
  \begin{itemize}
  \item Running the projection desk for services and meetings.
  \item Sound engineer for services and meetings,
  \item Events, Hospitality and Catering (including head caterer role),
  \item Wedding Verger (Paid casual work) Caretake for the church on the day of the wedding,
    \item Working with International Students to help them develop their English language skills, and help them learn about British culture.
  \end{itemize}
}
